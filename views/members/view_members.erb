<p class="table">Members:</p>

<% for member in @members %>
<div class="box">
  <p>Name: <%= member.first_name()%> <%= member.last_name() %></p>
  <p>Email: <%= member.email() %></p>

  <form class="" action="/average_joes/<%=member.id%>" method="get">
    <input type="submit" name="" value="View Member">
  </form>
  <br/>
<% end %>
</div>
